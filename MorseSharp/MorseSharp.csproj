<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>MorseSharp</Title>
    <Authors>p0laris</Authors>
    <Description>A simple lightweight .NET library to convert morse code.</Description>
    <PackageProjectUrl>https://github.com/p6laris/MorseSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/p6laris/MorseSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>MorseCode Morse Converter </PackageTags>
    <PackAsTool>False</PackAsTool>
    <NeutralLanguage>en-GB</NeutralLanguage>
    <Copyright>2022</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <IncludeSymbols>False</IncludeSymbols>
    <Version>2.0.2$(VersionPrefix)</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\OneDrive\Desktop\MorseSharp.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>


</Project>
